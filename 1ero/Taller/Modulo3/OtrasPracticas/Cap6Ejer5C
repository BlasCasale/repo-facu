programa Cap6Ejer5C
procesos
  proceso Escalon(E alto:numero; E ancho:numero)
  comenzar
    repetir alto
      mover
    derecha
    repetir ancho
      mover
    repetir 3
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    Escalon(3,2)
    Escalon(2,3)
    Escalon(1,4)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin