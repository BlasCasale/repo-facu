programa Cap6Ejer6A
procesos
  proceso RecorrerAv(E av:numero)
  comenzar
    Pos(av,1)
    repetir 99
      mover
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    RecorrerAv(5)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin