programa Cap5Ejer4A
procesos
  proceso reubicar
  comenzar
    Pos(PosAv,PosCa+4)
  fin
  proceso rectangulo
  comenzar
    repetir 2
      repetir 3
        mover
      derecha
      repetir 5
        mover
      derecha
  fin
  proceso final
  comenzar
    rectangulo
    repetir 2
      reubicar
      rectangulo
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    final
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin