programa Cap6Ejer5D
procesos
  proceso Escalon(E alto:numero; E ancho:numero; E prim:numero; E seg:numero)
  comenzar
    repetir alto
      mover
    repetir prim
      derecha
    repetir ancho
      mover
    repetir seg
      derecha
  fin
  proceso Abajo
  comenzar
    repetir 2
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    Pos(1,5)
    Escalon(2,4,1,3)
    Escalon(2,6,1,1)
    Escalon(4,2,3,1)
    repetir 4
      mover
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin  