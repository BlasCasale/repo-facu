programa Cap2Ejer9
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    Pos(1,17)
    derecha
    repetir 49
      si HayPapelEnLaBolsa
        depositarPapel
        mover
      mover
    depositarPapel
    mover
    Informar(PosAv,PosCa)
  fin
