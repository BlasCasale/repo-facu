programa Cap6Ejer1
procesos
  proceso Cuadrado(E lado:numero)
  comenzar
    repetir 4
      repetir lado
        mover
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    cantidad:numero
  comenzar
    cantidad:=8
    Cuadrado(cantidad)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin