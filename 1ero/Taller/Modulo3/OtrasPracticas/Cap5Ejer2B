programa Cap5Ejer2B
procesos
  proceso reubicar
  comenzar
    Pos(PosAv,PosCa+3)
  fin
  proceso cuadrado
  comenzar
    repetir 4
      mover
      mover
      derecha
  fin
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    cuadrado
    reubicar
    cuadrado
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin