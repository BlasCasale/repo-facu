programa Prueba3
areas
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    derecha
    mover
    si HayPapelEnLaBolsa
      depositarPapel
    si HayFlorEnLaBolsa
      depositarFlor
    Informar(PosAv, PosCa)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info,ciudad)
  Iniciar(R-info,1,1)
fin
